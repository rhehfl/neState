{
  "name": "@goooduyn/ne-state",
  "version": "1.0.2",
  "description": "React Context + useSyncExternalStore lightweight state manager with selector-based partial subscriptions.",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhehfl/neState.git"
  },
  "workspaces": [
    "src/*",
    "playground"
  ],
  "homepage": "https://github.com/rhehfl/neState",
  "bugs": {
    "url": "https://github.com/rhehfl/neState/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "state",
    "management",
    "store",
    "selector",
    "zustand",
    "redux",
    "lightweight",
    "performance",
    "리액트",
    "상태관리",
    "전역상태",
    "부분구독"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --out-dir dist --clean --sourcemap false --minify --treeshake --target es2018",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "devDependencies": {
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "packageManager": "yarn@4.3.1"
}
